{
    "name": "web36/e-fatura-entegrasyon",
    "type": "library",
    "version": "1.1.4",
    "description": "E fatura entegrasyonu için gerekli fonksiyonlar",
    "authors": [
      {
        "name": "Mehmet Çanak",
        "role": "Developer"
      }
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "homepage": "https://github.com/MehmetCanak/e-fatura-entegrasyon",
    "keywords" : ["json","laravel"],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "sabre/xml": "^2.1"
    },
    "autoload": {
      "psr-4": {
        "web36\\EFatura\\": "src/"
      },
      "files": [
        "src/helpers.php"
      ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload-dev": {
      "psr-4": {
        "web36\\EFatura\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "web36\\EFatura\\ServiceProvider"
        ]
      }
    },
    "config": {
      "discard-changes": true
    },
    "prefer-stable": true
  }
